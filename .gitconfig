[user]
	name = shun-tak
	email = shun.tak221@gmail.com
[color]
	ui = true
[alias]
	st = status -sb
	ci = commit
	cm = commit -m
	ciam = commit --amend
	co = checkout
	br = branch
	l = log
	ls = log --stat
	lp = log -p
	lo = log --pretty=format:'%ad %C(yellow)%h %Creset%an: %s' --date=short
	lg = log --graph --pretty=format:'%cd %C(yellow)%h %Creset%an: %s' --date=short
	last = log -1 HEAD
	df = diff HEAD
	dm = diff master
	cancel = reset --soft HEAD^
	sm = submodule
	smfe = submodule foreach
	spl = !git stash && git pull && git stash pop
[core]
	editor = emacs
	excludesfile = ~/.gitignore
[push]
	default = simple
[merge]
	ff = false
[pull]
	rebase = true
